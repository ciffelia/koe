    "conventional-changelog-conventionalcommits": "^7.0.2",
    "husky": "^8.0.3",
    "release-it": "^17.2.0",
    "semver": "^7.6.0"
  },
  "devDependencies": {
    "prettier": "^3.2.5"
  }
}
